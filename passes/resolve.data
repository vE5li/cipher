#function {

    resolve {

        block [ [ #single self ]
            #pass           [ #scope:self:block ] [ #scope:self:public ]
            #return         [ #scope:self ]
        ]

        module [ [ #single self ] [ #single public [ boolean ] ]
            #compile_module [ #root ] [ #scope:self:name ]
            #modify         [ #data #scope:data ] [ #last ]
            #return         [ #scope:self ]
        ]
    }
}
